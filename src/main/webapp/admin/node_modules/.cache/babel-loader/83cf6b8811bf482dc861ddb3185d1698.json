"请选择性别\">\r\n            <el-option\r\n                v-for=\"(item,index) in jiaoshixingbieOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshi'\"  label=\"职称\" prop=\"zhicheng\">\r\n          <el-select v-model=\"ruleForm.zhicheng\"  placeholder=\"请选择职称\">\r\n            <el-option\r\n                v-for=\"(item,index) in jiaoshizhichengOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='jiaoshi'\"  label=\"联系电话\" prop=\"lianxidianhua\">\r\n          <el-input v-model=\"ruleForm.lianxidianhua\"               placeholder=\"联系电话\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='shezhang'\"  label=\"社长账号\" prop=\"shezhangzhanghao\">\r\n          <el-input v-model=\"ruleForm.shezhangzhanghao\" readonly              placeholder=\"社长账号\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='shezhang'\"  label=\"社长姓名\" prop=\"shezhangxingming\">\r\n          <el-input v-model=\"ruleForm.shezhangxingming\"               placeholder=\"社长姓名\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"24\">  \r\n        <el-form-item v-if=\"flag=='shezhang'\" label=\"头像\" prop=\"touxiang\">\r\n          <file-upload\r\n          tip=\"点击上传头像\"\r\n          action=\"file/upload\"\r\n          :limit=\"3\"\r\n          :multiple=\"true\"\r\n          :fileUrls=\"ruleForm.touxiang?ruleForm